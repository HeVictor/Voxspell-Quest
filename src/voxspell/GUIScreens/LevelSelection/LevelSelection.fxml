<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="326.0" prefWidth="427.0" styleClass="root" stylesheets="@Background.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="voxspell.GUIScreens.LevelSelection.LevelSelectionController">
   <children>
      <VBox alignment="CENTER" layoutX="24.0" layoutY="84.0" spacing="20.0">
         <children>
            <Label text="Please select a level:" textFill="#2b194d">
               <font>
                  <Font name="System Bold" size="29.0" />
               </font>
            </Label>
            <ComboBox fx:id="levelSelect" onAction="#handleLevelSelect" prefHeight="41.0" prefWidth="186.0" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
      <Accordion layoutX="62.0" layoutY="29.0" />
      <Button fx:id="btnOK" layoutX="179.0" layoutY="239.0" mnemonicParsing="false" onAction="#handleBtnOK" prefHeight="26.0" prefWidth="70.0" text="OK">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
